    "cross-env": "^5.1.2",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "normalize.css": "^7.0.0",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^3.0.2",
